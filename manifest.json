{
  "name": "Internet Archive Explorer",
  "short_name": "IA Explorer",
  "id": "/",
  "start_url": "/",
  "scope": "/",
  "display": "standalone",
  "display_override": ["window-controls-overlay", "standalone"],
  "orientation": "any",
  "background_color": "#083344",
  "theme_color": "#06b6d4",
  "description": "A visually appealing and highly functional web application to browse, view, and discover content from the Internet Archive, enhanced with AI-powered summaries.",
  "icons": [
    {
      "src": "data:image/svg+xml;base64,",
      "sizes": "192x192",
      "type": "image/svg+xml",
      "purpose": "any"
    },
    {
      "src": "data:image/svg+xml;base64,",
      "sizes": "512x512",
      "type": "image/svg+xml",
      "purpose": "any maskable"
    }
  ],
  "categories": [
    "books_and_reference",
    "education",
    "entertainment",
    "social"
  ],
  "screenshots": [
    {
      "src": "https://storage.googleapis.com/aistudio-hosting/public/images/pwa_screenshots/ia_explorer_ss1.png",
      "sizes": "1080x1920",
      "type": "image/png",
      "form_factor": "narrow",
      "label": "Explore Page"
    },
    {
      "src": "https://storage.googleapis.com/aistudio-hosting/public/images/pwa_screenshots/ia_explorer_ss2.png",
      "sizes": "1080x1920",
      "type": "image/png",
      "form_factor": "narrow",
      "label": "Item Detail View"
    }
  ],
  "share_target": {
    "action": "/?view=webArchive",
    "method": "GET",
    "enctype": "application/x-www-form-urlencoded",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url"
    }
  },
  "shortcuts": [
    {
      "name": "My Library",
      "short_name": "Library",
      "description": "Access your saved items and collections",
      "url": "/?view=library",
      "icons": [{ "src": "data:image/svg+xml;base64,", "sizes": "192x192", "purpose": "any" }]
    },
    {
      "name": "Scriptorium",
      "short_name": "Scriptorium",
      "description": "Open your research workspace",
      "url": "/?view=scriptorium",
      "icons": [{ "src": "data:image/svg+xml;base64,", "sizes": "192x192", "purpose": "any" }]
    },
    {
      "name": "Search Archive",
      "short_name": "Search",
      "description": "Search the Internet Archive",
      "url": "/?view=explore",
      "icons": [{ "src": "data:image/svg+xml;base64,", "sizes": "192x192", "purpose": "any" }]
    }
  ],
  "prefer_related_applications": false
}